<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.jinlin.draggridview"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".custom_1st.Custom1stActivity"/>
        <activity android:name=".custom_2nd.Custom2ndActivity"/>
        <activity android:name=".custom_3rd.Custom3rdActivity"/>
        <activity android:name=".custom_4th.Custom4thActivity"/>
        <activity android:name=".custom_5th.Custom5thActivity"/>
        <activity android:name=".custom_6th.Custom6thActivity"/>
    </application>

</manifest>
